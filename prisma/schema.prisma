// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int    @id @default(autoincrement())
  object      String
  actor_id    String
  actor_name  String
  group       String
  // action String
  target_id   String
  target_name String
  location    String
  occurred_at String
  // metadata
}
